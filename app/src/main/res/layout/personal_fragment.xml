						android:textStyle="bold"
						android:textSize="30sp"
						android:layout_height="wrap_content"/>

				<TextView
						android:layout_width="wrap_content"
						android:text="帖子"
						android:textSize="12sp"
						android:textColor="#B0B0B0"
						android:layout_height="wrap_content"/>
			</LinearLayout>


			<LinearLayout

